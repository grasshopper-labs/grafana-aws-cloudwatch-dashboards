{
  "uid": "route53-healthcheck",
  "title": "Route 53 Health Check Monitoring",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 6,
  "refresh": "30s",
  "templating": {
    "list": [
      {
        "name": "environment",
        "label": "Environment",
        "type": "query",
        "datasource": "${DS_PROMETHEUS}",
        "query": "label_values(aws_route53_health_check_status, environment)",
        "multi": false,
        "includeAll": true,
        "allValue": ".*",
        "refresh": 2,
        "sort": 1
      },
      {
        "name": "partner_id",
        "label": "Partner",
        "type": "query",
        "datasource": "${DS_PROMETHEUS}",
        "query": "label_values(aws_route53_health_check_status{environment=~\"$environment\"}, partner_id)",
        "multi": false,
        "includeAll": true,
        "allValue": ".*",
        "refresh": 2,
        "sort": 1
      },
      {
        "name": "name",
        "label": "Tenant Domain",
        "type": "query",
        "datasource": "${DS_PROMETHEUS}",
        "query": "label_values(aws_route53_health_check_status{environment=~\"$environment\",partner_id=~\"$partner_id\"}, name)",
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "refresh": 2,
        "sort": 1
      }
    ]
  },
  "panels": [
    {
      "type": "gauge",
      "title": "Healthy Percentage (Global)",
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "avg(aws_route53_health_check_percentage_healthy{environment=~\"$environment\", partner_id=~\"$partner_id\", name=~\"$name\"})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 95 },
              { "color": "green", "value": 100 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Total Targets Monitored",
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "count(aws_route53_health_check_status{environment=~\"$environment\", partner_id=~\"$partner_id\", name=~\"$name\"})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0,
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "Healthy Percentage Trend",
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "aws_route53_health_check_percentage_healthy{environment=~\"$environment\", partner_id=~\"$partner_id\", name=~\"$name\"}",
          "legendFormat": "Global",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 4, "w": 8, "x": 16, "y": 0 },
      "options": {
        "legend": { "showLegend": false },
        "tooltip": { "mode": "all" },
        "standardOptions": {
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Health Check Status Over Time",
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "aws_route53_health_check_status{environment=~\"$environment\", partner_id=~\"$partner_id\", name=~\"$name\"}",
          "legendFormat": "{{name}}",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 4 },
      "options": {
        "legend": { "showLegend": true },
        "tooltip": { "mode": "all" },
        "standardOptions": {
          "min": 0,
          "max": 1
        }
      }
    },
    {
      "type": "table",
      "title": "Health Check Targets Overview",
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "aws_route53_health_check_status{environment=~\"$environment\", partner_id=~\"$partner_id\", name=~\"$name\"}",
          "legendFormat": "{{name}}",
          "refId": "A"
        }
      ],
      "transformations": [
        {
          "id": "labelsToFields",
          "options": {
            "mode": "labels"
          }
        },
        {
          "id": "organize",
          "options": {
            "columns": [
              { "text": "partner_id", "field": "partner_id" },
              { "text": "environment", "field": "environment" },
              { "text": "name", "field": "name" },
              { "text": "stat", "field": "stat" },
              { "text": "Status", "field": "Value" }
            ]
          }
        }
      ],
      "options": {
        "showHeader": true
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": { "text": "Down" },
                "1": { "text": "Up" }
              }
            }
          ],
          "custom": {}
        },
        "overrides": []
      },
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 14 }
    }
  ],
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ]
}